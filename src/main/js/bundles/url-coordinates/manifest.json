            "impl": "./URLCoordinatesHandler",
            "provides": [
                "parametermanager.Parametrizable"
            ],
            "propertiesConstructor": true,
            "properties": {
                "verboseInput": true,
                "highlightCenter": true
            },
            "references": [
                {
                    "name": "_mapWidgetModel",
                    "providing": "map-widget.MapWidgetModel"
                },
                {
                    "name": "_highlighter",
                    "providing": "highlights.HighlightService"
                }
            ]
        }
    ]
}
