{
    "name": "dn_urlcoordinates",
    "version": "1.0.0",
    "title": "${bundleName}",
    "description": "${bundleDescription}",
    "vendor": "con terra GmbH",
    "productName": "devnet-mapapps-url-coordinates",
    "keywords": [
        "url",
        "coordinates",
        "url parameters"
    ],
    "layer": "module",
    "i18n": [
        "bundle"
    ],
    "dependencies": {
        "parametermanager": "^4.13.0"
    },
    "components": [
        {
